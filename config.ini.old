[general]
# Output folder for CSVs (relative to project root)
output_dir = data
# Max concurrent requests when fetching member detail pages
concurrency = 8
# Request timeout (seconds)
timeout = 20
# If a member detail page fails, retry this many times with exponential backoff
max_retries = 3
# Respect robots.txt? (false = ignore). NOTE: Keep true for best practice.
respect_robots = true
# Optional: custom user agent (leave blank to use default)
user_agent =

[targets]
# One or more list URLs, separated by newlines.
# You can include other ChamberMaster list pages here as needed.
urls =
